{
  "name": "node-extra-ca-certs-vscode",
  "displayName": "Node Extra CA Certs VSCode",
  "description": "Make all NODE_EXTRA_CA_CERTS available to VS Code extensions",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "publisher": "pharndt",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philipparndt/node-extra-ca-certs-vscode"
  },
  "bugs": {
    "url": "https://github.com/philipparndt/node-extra-ca-certs-vscode"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts --max-warnings=0",
    "lint:fix": "eslint src --ext ts --max-warnings=0 --max-warnings=0 --fix",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/mocha": "10.0.1",
    "@types/node": "18.15.11",
    "@types/vscode": "1.77.0",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vscode/test-electron": "2.3.0",
    "eslint": "8.38.0",
    "eslint-config-standard": "17.0.0",
    "glob": "10.1.0",
    "mocha": "10.2.0",
    "ts-loader": "9.4.2",
    "typescript": "4.6.4",
    "webpack": "5.79.0",
    "webpack-cli": "5.0.1"
  }
}
